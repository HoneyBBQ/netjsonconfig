package system

config system 'system'
	option buffer_size_kb '256'
	option console_log_level '6'
	option cron_log_level '5'
	option description 'demo config'
	option hostname 'owrt-demo'
	option kernel_console_log_level '7'
	option log_buffer_size_kb '128'
	option log_file '/tmp/system.log'
	option log_hostname 'log-host'
	option log_ip '192.0.2.10'
	option log_port '1514'
	option log_prefix 'demo'
	option log_protocol 'udp'
	option log_remote '1'
	option log_size_kb '64'
	option log_trailer_null '1'
	option log_type 'circular'
	option maintainer 'ops@example.com'
	option notes 'unit test'
	option timezone 'UTC'
	option tty_login_required '1'
	option urandom_seed_enabled '1'
	option zonename 'UTC'
	option zram_compression 'zstd'
	option zram_size_mb '32'

config timeserver 'ntp'
	option enable_server '1'
	option enabled '1'
	option hostname 'ntp.local'
	option port '123'
	list pools 'pool.ntp.org'
	list servers '0.pool.ntp.org'
	list servers '1.pool.ntp.org'

config led 'led_led_usb'
	option default '1'
	option delayoff '100'
	option delayon '200'
	option dev 'usb0'
	option interval '50'
	option message 'usb'
	option mode 'link'
	option name 'led-usb'
	option sysfs 'tp-link:blue:usb'
	option trigger 'usb'

