{
  "general": {
    "hostname": "test-system"
  },
  "leds": [
    {
      "name": "USB1",
      "sysfs": "tp-link:green:usb1",
      "trigger": "usbdev",
      "dev": "1-1.1",
      "interval": 50
    },
    {
      "name": "WLAN2G",
      "sysfs": "tp-link:blue:wlan2g",
      "trigger": "phy0tpt",
      "default": false,
      "delayoff": 1,
      "delayon": 5
    },
    {
      "name": "USB2",
      "sysfs": "tp-link:blue:usb2",
      "trigger": "usbdev",
      "dev": "1-1.2",
      "interval": 50
    }
  ]
}

